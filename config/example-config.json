{
  "applications": {
    "msedge.exe": [
      {
        "regex": "\\w*.pdf",
        "comment": "Configuration for browser PDF files reading",
        "configuration": {
          "button_combinations": [
            {
              "combination": "A",
              "action": "KeyDown Enter"
            },
            {
              "combination": "LS+RS",
              "action": "KeyDown F11"
            },
            {
              "combination": "DPadUp",
              "action": "KeyDown Up"
            },
            {
              "combination": "DPadDown",
              "action": "KeyDown Down"
            },
            {
              "combination": "LB",
              "action":  "KeyDown VolumeDown"
            }
          ]
        }
      }
    ]
  }
}
